{
  "name": "vite2vue3study",
  "version": "0.0.1",
  "homepage": "https://[username].github.io/vue3Init",
  "scripts": {
    "dev": "vite serve --mode development",
    "test:unit": "jest",
    "test:unit-coverage": "jest --coverage",
    "test:gzip": "npx http-server dist --cors --gzip -c-1",
    "ppe": "vite serve --mode ppe",
    "prod": "vite serve --mode production",
    "serve": "vite preview",
    "build": "vite build",
    "build:dev": "vue-tsc --noEmit && vite build --mode development",
    "build:test": "vue-tsc --noEmit && vite build --mode test",
    "build:ppe": "vue-tsc --noEmit && vite build --mode ppe",
    "build:prod": "vue-tsc --noEmit && vite build --mode production",
		"fix": "eslint --fix --ext .js,.vue src",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore . --fix",
    "lint:css": "stylelint **/*.{html,vue,css,sass,scss,less}"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^1.1.1",
    "@vueuse/core": "^7.5.4",
