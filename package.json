    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^7.0.0",
		"http-proxy-middleware": "^2.0.6",
		"ts-jest": "^27.1.2",
    "jest": "^27.4.5",
    "husky": "^2.3.0",
    "less": "^4.1.2",
    "lint-staged": "^8.1.7",
    "mockjs": "^1.1.0",
    "postcss": "^8.4.5",
