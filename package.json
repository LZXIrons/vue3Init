    "loadsh": "^0.0.4",
    "pinia": "^2.0.11",
    "qs": "^6.10.3",
    "vue": "^3.2.6",
    "vue-clipboard3": "^1.0.1",
    "vue-router": "^4.0.11",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
